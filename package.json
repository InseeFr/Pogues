  "scripts": {
    "test": "jest --coverage -i",
    "testw": "jest --watch",
    "dev": "webpack-dev-server",
    "dev:dev": "cross-env webpack-dev-server --env.environment=dev",
    "dev:server": "concurrently \"npm run server\" \"npm run dev:dev\"",
    "server": "node ./build-config/fake-backend/server.js",
    "build": "cross-env webpack --display-error-details --env.prod=true --env.environment=prod",
    "build:dev": "cross-env webpack --env.prod=true --env.environment=dev",
    "build:dv": "cross-env webpack --env.prod=true --env.environment=dv",
    "build:preprod": "cross-env webpack --env.prod=true --env.environment=preprod",
    "build:qa": "cross-env webpack --env.prod=true --env.environment=qa",
    "build:qf": "cross-env webpack --env.prod=true --env.environment=qf",
    "build:travis": "cross-env webpack --env.prod=true --env.environment=dev",
    "lint": "eslint ./src -f table || true",
    "lint:fix": "eslint ./src -f table --fix || true",
    "start": "npm run dev",
    "jison": "jison jison/grammars/xpath.jison jison/grammars/xpath.jisonlex -o jison/generated-parsers/xpath-parser.js && node ./jison/fixParser.js",
    "format": "prettier --write \"{src,build-config,e2e}/**/*.{js,jsx,css,scss}\"",
    "pre-commit": "npm run lint && npm run test"
  },
  "author": "INSEE (http://www.insee.fr)",
  "husky": {
    "hooks": {
      "pre-push": "yarn pre-commit"
    }
  },
  "contributors": [
