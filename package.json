    }
  ],
  "license": "MIT",
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ],
    "moduleNameMapper": {
      "^Config$": "<rootDir>/build-config/environments/config.prod.js",
      "\\.(css|scss|png)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/jison/",
      "<rootDir>/_book/"
    ],
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}"
    ],
    "testURL": "http://localhost/"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  }
